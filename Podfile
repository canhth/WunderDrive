platform :ios, '9.0'

target 'WunderDrive' do
  use_frameworks!

  inherit! :search_paths
    pod 'Alamofire' 
    pod 'RxSwift'
    pod 'RxCocoa' 
    pod 'SwiftyJSON'
    pod 'ObjectMapper' 
    pod 'KeychainAccess' 
    pod 'AMScrollingNavbar'
    pod 'SVProgressHUD', '2.1.2'
    pod 'CT_RESTAPI', :path => 'LocalPods/CT_RESTAPI/'
    pod 'MapViewPlus'

  target 'WunderDriveTests' do
    inherit! :search_paths
    # Pods for testing
        pod 'Quick', '~> 1.2'
        pod 'Nimble', '~> 7.0'
  end

end
